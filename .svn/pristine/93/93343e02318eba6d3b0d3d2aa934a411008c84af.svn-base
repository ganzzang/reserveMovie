package com.cinema.web.movie.bean;

// 좌석정보 관리하는 Bean
public class SeatBean {
	
	private int seatNo;
	private String seatValue;
	private String code;
	private String id;
	private String revRequestSeat;
	private String[] revRequestSeatArr;
	private String revRequestShowSeat;
	private String[] revRequestShowSeatArr;
	
	
	/* version 68
	public SeatBean(){
		
	};	public SeatBean(int seatNo, String seatValue, String code){
		this.seatNo = seatNo;   
		this.seatValue = seatValue;
		this.code = code;     
	}*/	
	public int getSeatNo() {
		return seatNo;
	}
	public void setSeatNo(int seatNo) {
		this.seatNo = seatNo;
	}
	public String getSeatValue() {
		return seatValue;
	}
	public void setSeatValue(String seatValue) {
		this.seatValue = seatValue;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getRevRequestSeat() {
		return revRequestSeat;
	}
	public void setRevRequestSeat(String revRequestSeat) {
		this.revRequestSeat = revRequestSeat;
	}
	public String[] getRevRequestSeatArr() {
		return revRequestSeatArr;
	}
	public void setRevRequestSeatArr(String[] revRequestSeatArr) {
		this.revRequestSeatArr = revRequestSeatArr;
	}

	public String getRevRequestShowSeat() {
		return revRequestShowSeat;
	}

	public void setRevRequestShowSeat(String revRequestShowSeat) {
		this.revRequestShowSeat = revRequestShowSeat;
	}

	public String[] getRevRequestShowSeatArr() {
		return revRequestShowSeatArr;
	}

	public void setRevRequestShowSeatArr(String[] revRequestShowSeatArr) {
		this.revRequestShowSeatArr = revRequestShowSeatArr;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}

	
	
}
